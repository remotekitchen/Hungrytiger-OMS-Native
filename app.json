{
  "expo": {
    "name": "HungryTiger-OMS",
    "slug": "HungryTiger-OMS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main_icon_300x300.png",
    "scheme": "hungrytigeroms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.chatchefs.htoms",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.chatchefs.htoms",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_icon_300x300.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.NOTIFICATIONS",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/main_icon_300x300.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Hungry Tiger to access your photos to select menu item images.",
          "cameraPermission": "Allow Hungry Tiger to access your camera to take menu item photos."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Hungry Tiger to access your camera",
          "microphonePermission": "Allow Hungry Tiger to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main_icon_300x300.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow HungryTiger-OMS to access your microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "6ddee96b-b374-4bf5-8a45-e9c1bbc56e52"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6ddee96b-b374-4bf5-8a45-e9c1bbc56e52"
    }
  },
  "submit": {
    "Production": {
      "ios": {
        "appleId": "siliconvalleystan@gmail.com",
        "ascAppId": "6749886403",
        "appleTeamId": "SRQSDLH6K4"
      }
    }
  }
}
